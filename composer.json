    "homepage": "https://github.com/zorhand/region",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "region",
        "thinkphp",
        "省市区"
    ],
    "authors": [
        {
            "name": "yupoxiong",
            "email": "i@yupoxiong.com",
            "homepage": "https://www.yupoxiong.com"
